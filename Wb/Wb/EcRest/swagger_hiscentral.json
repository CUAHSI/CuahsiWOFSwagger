                    "parameters": [
                        {
                            "name": "xmin",
                            "description": "xmin, try -114",
                            "required": "required",
                            "type": "string",
                            "paramType": "query",
                            "defaultValue": "-114"
                        },
						{
                            "name": "xmax",
