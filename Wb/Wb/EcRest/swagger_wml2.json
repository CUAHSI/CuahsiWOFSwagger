{
    "apiVersion": "1.0.0",
    "swaggerVersion": "1.2",
    "info":  {
  "title": "CUAHSI HIS WaterOneFloe Web Services",
    "description": "WaterOneFlow: using the series and service information, retrieve data from distributed data services. More information at <a href='http://his.cuahsi.org/wofws.html'>HIS Website</a>",


  "contact": "busmgr@cuahsi.org"
  
},
    "basePath": "http://localhost:6278/genericODws/REST/",
    "resourcePath": "/",
    "produces": [
        "text/xml"
    ],
    "authorizations": {},
    "apis": [
        
        {
            "path": "/waterml_2.svc/featureOfInterest",
            
            "operations": [
                {
                    "httpMethod": "GET",
                    "method": "GET",
                    "summary": "Get siteListing",
                    "produces": [
                        "text/xml"
                    ],
                    "nickname": "Sites",
                    "authorizations": {},
                    "parameters": [
                        {
                            "name": "location",
                            "description": "site identifier",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        }
                    ],
                    "responseMessages": [
                        
                        {
                            "code": 404,
                            "message": "Invalid location specified"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/waterml_2.svc/observedProperty",
            
            "operations": [
                {
                    "httpMethod": "GET",
                    "method": "GET",
                    "summary": "Get Variables",
                    "produces": [
                        "text/xml"
                    ],
                    "nickname": "Variables",
                    "authorizations": {},
                    "parameters": [
                        {
                            "name": "variable",
                            "description": "variable",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        }
                    ],
                    "responseMessages": [
                        
                        {
                            "code": 404,
                            "message": "Invalid location specified"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/waterml_2.svc/values",
            
            "operations": [
                {
                    "httpMethod": "GET",
                    "method": "GET",
                    "summary": "Get WaterML2 timeseries",
                    "produces": [
                        "text/xml"
                    ],
                    "nickname": "Values",
                    "authorizations": {},
                    "parameters": [
                        {
                            "name": "location",
                            "description": "site identifier",
                            "required": true,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "variable",
                            "description": "paramter code",
                            "required": true,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "startDate",
                            "description": "The southernmost latitude",
                            "required": true,
                            "type": "dateTime",
                            "paramType": "query"
                        },
                        {
                            "name": "endDate",
                            "description": "The southernmost latitude",
                            "required": true,
                            "type": "dateTime",
                            "paramType": "query"
                        }
                    ],
                    "responseMessages": [
                        
                        {
                            "code": 404,
                            "message": "Invalid location specified"
                        }
                    ]
                }
            ]
        }
    ]
}

